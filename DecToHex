h=[str(i) for i in range(10)]+["A","B","C","D","E","F"]
def hexa(dec):
    s=" "
    while dec:
        s+=h[dec%16]
        dec//=16
    return s[::-1]
print(hexa(23665))
print(hexa(65537))



